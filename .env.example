# AAI SYSTEM CONFIGURATION - TEMPLATE
# Copy this file to .env and fill in your actual API keys

# OpenAI Integration
OPENAI_API_KEY=your-openai-api-key-here

# OpenRouter API - MULTIPLE API GATEWAY
OPENROUTER_API_KEY=your-openrouter-api-key-here

# Discord Integration
DISCORD_BOT_TOKEN_OLYMPUS=your-discord-bot-token-here
DISCORD_BOT_TOKEN_GROUND_ZERO=your-ground-zero-token-here

# Google OAuth
GOOGLE_CLIENT_ID=your-google-client-id-here
GOOGLE_CLIENT_SECRET=your-google-client-secret-here

# Supabase Database (Project Olympus)
SUPABASE_URL=https://your-project-id.supabase.co
SUPABASE_ANON_KEY=your-anon-key-here
SUPABASE_SERVICE_KEY=your-service-key-here
SUPABASE_PASSWORD=your-database-password-here

# AAI Brain Cache Configuration
BRAIN_CACHE_TABLE=aai_cache_entries
BRAIN_STATES_TABLE=aai_conversation_states
BRAIN_SEARCH_TABLE=aai_cache_search_index
BRAIN_TAGS_TABLE=aai_cache_tags
BRAIN_SEARCH_ENABLED=true

# Application Settings
NODE_ENV=development
PORT=3000
LOG_LEVEL=debug

# Security Configuration
JWT_SECRET=your-super-secret-jwt-key-change-in-production
JWT_REFRESH_SECRET=your-super-secret-refresh-key-change-in-production
BCRYPT_SALT_ROUNDS=12

# API Rate Limiting
API_RATE_LIMIT_WINDOW_MS=900000
API_RATE_LIMIT_MAX_REQUESTS=100

# AI Configuration
AI_REQUEST_TIMEOUT_MS=30000
AI_MAX_RETRIES=3
AI_FALLBACK_MODEL=openai/gpt-4o-mini

# Brain System Configuration
BRAIN_AUTO_BACKUP=true
BRAIN_VERSION_RETENTION=30
BRAIN_LOG_INTERACTIONS=true
BRAIN_CACHE_MAX_SIZE=1000000

# Research & Context Engineering
RESEARCH_AUTO_SUMMARIZE=true
JINA_SCRAPE_TIMEOUT=30000
DOCUMENTATION_CACHE_TTL=86400